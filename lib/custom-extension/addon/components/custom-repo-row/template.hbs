<td valign="middle" class="row-check" style="padding-top: 2px;">
  {{check-box nodeId=model.id}}
</td>
<td data-title="{{dt.name}}">
  {{#link-to 'image-repo.projects.detail.repository' model.project_id currentUserRoleId model.name class="clip project-link"}}
    {{model.name}}
  {{/link-to}}
</td>
<td data-title="{{dt.tags_count}}">
  {{model.tags_count}}
</td>
<td data-title="{{dt.pull_count}}">
  {{model.pull_count}}
</td>
<td data-title="{{dt.actions}}" class="actions">
  {{custom-action-menu model=model resourceActions=resourceActions menuClick=(action 'handleMenuClick')}}
</td>
