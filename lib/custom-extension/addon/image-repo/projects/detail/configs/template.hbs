<section style="width: 100%;">
  <div style="font-size: 1.5em;height: 30px;" class="clip">{{model.repo.name}}</div>
</section>
<section class="header has-tabs clearfix pb-0">
  <ul class="tab-nav">
    {{#if model.harborVersion.supportSummary}}
      <li>
        {{#link-to "image-repo.projects.detail.summary" model.projectId model.currentUserRoleId}}{{t "harborConfig.tab.summary"}}{{/link-to}}
      </li>
    {{/if}}
    <li>
      {{#link-to "image-repo.projects.detail.repositories" model.projectId model.currentUserRoleId}}{{t "harborConfig.tab.store"}}{{/link-to}}
    </li>
    {{#if (or isSystemAdmin isMember)}}
    <li>
      {{#link-to "image-repo.projects.detail.members" model.projectId model.currentUserRoleId}}{{t "harborConfig.tab.member"}}{{/link-to}}
    </li>
    {{/if}}
    {{!-- <li>
      {{#link-to "image-repo.projects.detail.replications" model.projectId}}复制{{/link-to}}
    </li> --}}
    {{#if (or isProjectAdmin isProjectMaster isSystemAdmin)}}
    <li>
      {{#link-to "image-repo.projects.detail.labels" model.projectId model.currentUserRoleId}}{{t "harborConfig.tab.tag"}}{{/link-to}}
    </li>
    {{/if}}
    {{#if (or isSystemAdmin isMember)}}
    <li>
      {{#link-to "image-repo.projects.detail.logs" model.projectId model.currentUserRoleId}}{{t "harborConfig.tab.log"}}{{/link-to}}
    </li>
    {{/if}}
    {{#if (or isSystemAdmin isMember)}}
    <li>
      {{#link-to "image-repo.projects.detail.configs" model.projectId model.currentUserRoleId}}{{t "harborConfig.tab.accessLevel"}}{{/link-to}}
    </li>
    {{/if}}
  </ul>
</section>
<section>
  <div class="fixed-header-actions clearfix">
    <div style="position: absolute; right: 0px; top: 62px;">
    {{#if config.public}}
      <div class="btn-group no-inline-space">
        <button disabled={{not isProjectAdmin}} class="btn btn-link btn-sm bg-default" {{action "changePublic" false}}>{{t "harborConfig.table.storeStatus.private"}}</button>
        <button class="btn btn-link btn-sm bg-success">{{t "harborConfig.table.storeStatus.public"}}</button>
      </div>
    {{else}}
      <div class="btn-group no-inline-space">
        <button class="btn btn-link btn-sm bg-primary">{{t "harborConfig.table.storeStatus.private"}}</button>
        <button disabled={{not isProjectAdmin}} class="btn btn-link btn-sm bg-defualt" {{action "changePublic" true}}>{{t "harborConfig.table.storeStatus.public"}}</button>
      </div>
    {{/if}}
    </div>
  {{#banner-message color="bg-info"}}
    <p>
      {{t "harborConfig.table.storeStatus.message"}}
    </p>
  {{/banner-message}}

  {{save-cancel save=(action "savePublicConfig") saveDisabled=saveDisabled cancelDisabled=true createLabel="imageRepoSection.adminConfigPage.saveHarborConfig" }}
  </div>
    {{!-- <div class="pull-left" style="margin-left:10px;padding-left:20px;font-size:17px">
      项目仓库
    </div>
    <div class="pull-left" style="flex:1;padding-left:180px;">
      <div class="row">
        <label class="acc-label" for="plubic" style="font-size: 15px; color: #454545;user-select: none;">公开</label>
        {{#input-or-display value=openOrTransparent}}
          {{input type="checkbox" id="plubic" name="plubic" class="form-control" checked=config.public placeholder="是否公开"}}
        {{/input-or-display}}
        <div class="text-info" style="margin-top:5px;"> 所有人都可访问公开的项目仓库。</div>
      </div>
    </div>
  </div>
  {{save-cancel save=(action "savePublicConfig") saving=saving saveDisabled=saveDisabled cancelDisabled=cancelDisabled cancel="cancelPublicConfig" createLabel="imageRepoSection.adminConfigPage.saveHarborConfig" }} --}}
</section>
